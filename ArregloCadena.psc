Algoritmo ArregloCadena
	Definir valoresArreglo Como Cadena
	Definir dimensionArreglo Como Entero
	Imprimir "Ingrese la dimension"
	leer dimensionArreglo
	
	Dimension valoresArreglo[dimensionArreglo]
	Para i = dimensionArreglo - 1 Hasta 0 Con Paso -1 Hacer
		Imprimir "Ingrese nombres"
		Leer valoresArreglo[i]
		Imprimir valoresArreglo[i]
	Fin Para
	
FinAlgoritmo
