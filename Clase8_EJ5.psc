Algoritmo Clase8_EJ5
	
	Imprimir "Ingrese un número"
	leer num1
	Imprimir "Ingrese otro número"
	leer num2
	
	Si num1 = num2 Entonces
		Imprimir "Los dos números son iguales"
	SiNo
		Si num1 > num2 Entonces
			Imprimir num1 " es mayor que " num2
		SiNo
			Imprimir num2 " es mayor que " num1
		Fin Si
	Fin Si
	
FinAlgoritmo
